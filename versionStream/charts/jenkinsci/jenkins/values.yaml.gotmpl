master:
  JCasC:
    enabled: true

  additionalPlugins:
  - jx-resources:1.0.38

  ingress:
    enabled: true
{{- if .Values.jxRequirements.ingress.domain }}
    hostName: "jenkins{{ .Values.jxRequirements.ingress.namespaceSubDomain | default "." }}{{ .Values.jxRequirements.ingress.domain }}"
{{- end }}
  sidecars:
    configAutoReload:
      enabled: true
rbac:
  create: true